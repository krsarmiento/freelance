{% for achievement in achievements %}
    <div class="media">
        <a class="pull-left" href="{{ achievement.url }}" target="_blank">
          <img class="media-object" data-src="holder.js/64x64" alt="64x64" src="data:image/svg+xml;base64," style="width: 64px; height: 64px;">
        </a>
        <div class="media-body">
          <h4 class="media-heading">{{ achievement.title }}</h4>
          {{ achievement.description }}
        </div>
    </div>
{% endfor %}